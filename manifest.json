{
    "manifest_version": 2,
    "name": "HideNicoVideoNewsForChrome",
    "version": "1.0",
    "description": "Hides news bar of Nicovideo player.",
    "permissions": ["http://www.nicovideo.jp/*"],
    "content_scripts": [
        {
            "matches": ["http://www.nicovideo.jp/*"],
            "css": ["removenews.css"]
        }
    ]
}
